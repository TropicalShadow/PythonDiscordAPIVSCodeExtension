{
    "main cog":[
        "from discord.ext.commands import Cog, Bot",
        "from discord import slash_command",
        "",
        "class Template(Cog):",
        "    def __init__(self, bot: Bot):",
        "        self.bot = bot",
        "",
        "    @slash_command(name=\"ping\")",
        "    async def ping_command(self, ctx):",
        "        await ctx.respond(\"pong\")",
        "",
        "",
        "def setup(bot: Bot):",
        "    bot.add_cog(Template(bot))"
    ]
}